

/* register map */

r0 = 
r1 = one
r2 = temp
r3 = mask3
r4 = pa
r5 = mask5
r6 = mask6
r7 = mask7
r8 = mask8
r9 = idx
rA = m0
rB = m1
rC = m2
rD = m3
rE = &main
rF = 

loadaddr one, 1 // 7101
loadaddr mask3, 3  // 7303
loadaddr mask5, 5 // 7505
loadaddr mask6, 6 // 7606
loadaddr mask7, 7 // 7707
loadaddr mask8, 8 (0x1000) to check for negative numbers // 7808
loadaddr ra, &main // 7EXX

main
	store m0, 0xFF // 9AFF
	and temp, m0, mask8 /* read first bit of input */ // 32A8
	bpos temp, halt // D2XX

	store m1, 0xFF // 9BFF
	store m2, 0xFF // 9CFF
	store m3, 0xFF // 9DFF
	
	loadaddr pa, 0 // 7400
	loadaddr idx, 0 // 7900
	(pread)
	store temp, 0xFF // 92FF 
	sl temp, temp, idx // 5229
	xor pa, pa, temp
	add idx, idx, one
	sub temp, mask3, idx
	bpos temp, pread
	
	(pcorrectmod)
	xor temp, pa, mask3
	bpos temp, flipm1 // ( mask != pa, go to next flip)
	xor m0, m0, 1
	
	(flipm1)
	xor temp, pa, mask5
	bpos temp, flipm2
	xor m1, m1, 1
	
	(flipm2)
	xor temp, pa, mask6
	bpos temp, flipm3
	xor m2, m2, 1
	
	(flipm3)
	xor temp, pa, mask7
	bpos temp, print
	xor m3, m3, 1
	
	(print)
	load m0, 0xFF
	load m1, 0xFF
	load m2, 0xFF
	load m3, 0xFF
	ja main
	
halt:
	0000
