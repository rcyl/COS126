pa = 0
mor = 0

store m1
mor = mor ^ m1

store m2
mor = mor ^ m2

store m3
mor = mor ^ m2


calc p1 = mor xor m3
get p1 from stdin
compare stdin p1 with calc p1
if (!zero) 
	pa << (1 << 2)
	pa += pa

calc p2 = ma xor m2
get p2 from stdin
compare stdin p2 with calc p2
if (!zero)
	pa << (1 << 1)
	pa += p2

calc p3 = ma xor m1
get p3 from stdin
compare stdin p3 with calc p3
if (!zero)
	pa << (1 << 0)
	pa += p3
	
xor ma with pa

print m1
print m2
print m3
print m4


