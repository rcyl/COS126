#include <iostream>

int main() {

	int M[4] = { 1, 0, 0, 1};
	int p[3] = { 1, 0, 0 };
	int mor = 0;
	int calcpi;
	int cmpi;
	int failCount = 0;

	for(int i= 0; i < 4; i++) {
		mor ^= M[i];
	}

	for(int i = 2; i >=0 ; i--) {
		int pi = p[2 - i];
		calcpi = mor ^ M[2 - i];
		cmpi = (calcpi == pi);
	}



	return 0;
}


