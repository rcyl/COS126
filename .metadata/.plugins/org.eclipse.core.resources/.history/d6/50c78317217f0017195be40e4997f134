pcorrectmod
	xor temp, pa, mask3
	bzero temp, flipm0
	xor temp, pa, mask3
	bzero temp, flipm0
	xor temp, pa, mask3
	bzero temp, flipm0
	xor temp, pa, mask3
	bzero temp, flipm0

flipm0
	xor m0, m0, 1
	print

flipm0
	xor m1, m1, 1
	print

flipm2
	xor m2, m1, 1
	print

flipm3
	xor m3, m0, 1
	print

print 
	store m0, 0xFF
	store m1, 0xFF
	store m2, 0xFF
	store m3, 0xFF