edited 24th july 2018

/* register map */

r0 = 0
r1 = one
r2 = two
r3 = three
r4 = mi
r5 = rpi
r6 = calpi
r7 = cmpi
r8 = i
r9 = mor
rA = tmp
rB = 
rC = 
rD = 
rE = F7
rF = 

p1 = (m1 ^ m2 ^ m3 ^ m4) ^ m3 =
		m1 ^ m2 ^ m4 

store m1 to m4 in array
xor m1 to m4 together to get mor

pi => read parity
calcpi => calculated parity
cmpi => compared parity

set failcount = 0
set i == 2
read pi
get calcpi by xor mor ^ m[2 - i]
get cmpi by comparing pi with calcpi
if cmpi == 1, set failcount + 1 flip M[2 - i] aka M[2 - i] ^ 1
i--
repeat till i = 0
if failcount == 3,  flip M[3] 
		

****/
cmpi 	= 0
mor 	= 0
one		= 1
two 	= 2
three 	= 3
maskF7 	= F7

main:
	i = 3
get: 
	mi => read frm stdin
	temp = mi & F7
	bpos temp halt // if any bit except bit 0 is positive, this will halt
	mor =>  mor ^ mi
	temp = 3 - i
	M[temp] = mi
	i = i - 1
	bpos i, get
	i = 2		
flip: 
	rpi => read from stdin
	tmp = 2 - i
	mi = M[tmp]
	calpi = mor ^ mi 
	temp = (calcpi == rpi)
	temp = cmpi << i
	cmpi = cmpi + temp
	bpos i, flip
	i = 2
calc:
	temp = (1 << i)
	temp = (temp == cmpi)
	bpos temp, calc
	
print:	
	print mi
	j main
	
halt:
	0
		


*/

