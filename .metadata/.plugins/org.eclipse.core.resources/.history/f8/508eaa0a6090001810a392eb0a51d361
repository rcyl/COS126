#include <iostream>

int main() {

	int M[4] = { 1, 0, 0, 1};
	int p[3] = { 1, 0, 0 };
	int mor = 0;
	int calcpi;
	int cmpi;
	int failCount = 0;

	for(int i= 0; i < 4; i++) {
		mor ^= M[i];
	}


	for(int i = 2; i >=0 ; i--) {
		int pi = p[2 - i];
		calcpi = mor ^ M[i];
		cmpi = (calcpi == pi);
		if (cmpi == 1) {
			failCount++;
			M[2 - i] = M[i] ^ 1;
		}
	}

	if (failCount == 3) {
		M[3] = M[3] ^ 1;
	}

	for(int i = 0; i < 4; i++) {
		std::cout << M[i] << std::endl;
	}


	return 0;
}


