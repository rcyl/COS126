#include <QApplication>
#include "sierpinski.h"

int main(int argc, char *argv[]) {

	QApplication app(argc, argv);

	Sierpinski window(100, 3);

	window.setWindowTitle("Sierpinski");
	window.resize(1280, 960);
	window.show();

	return app.exec();
}


