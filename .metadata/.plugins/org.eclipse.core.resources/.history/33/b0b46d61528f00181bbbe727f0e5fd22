pa = 0 => mask of bits to be corrected /* p1 p2 p3 */
mor = 0 => xor of all input msg bits
ma = 0 => bit shifted sum of all msg bits /* m4 m3 m2 m1 */ 

get&shift(0)
get&shift(1)
get&shift(2)
get&shift(3)

p1 = (m1 ^ m2 ^ m3 ^ m4) ^ m3 =
		m1 ^ m2 ^ m4 
comp&shift(2) // m3 is at bit 2
comp&shift(1)
comp&shift(0)

get&shift(shift):
	input_mask => read frm stdin
	mor =>  mor ^ input_mask
	temp = (1 << shift)
	ma += ma + temp
		
comp&shift(shift):
	input_parity => read from stdin
	mi = ma >> shift 
	mi = temp & 1
	calc_parity = mor ^ mask 
	temp = (calc_parity == recv parity)
	if temp == 0
		temp << (1 << shift)
		pa += temp 
		
print(shift)
	temp = ma >> shift
	temp = temp & 1
	print temp
	
/* to get flip mask */	
ma = ma ^ pa

print(0)
print(1)
print(2)
print(3)



