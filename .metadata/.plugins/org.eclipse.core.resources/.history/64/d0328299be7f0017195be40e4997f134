
loadaddr one, 1
loadaddr mask3, 3 
loadaddr mask5, 5
loadaddr mask6, 6
loadaddr mask7, 7
loadaddr mask8, 8 (0x1000) to check for negative numbers

main
	load m0, 0xFF
	xor temp, m0, mask8
	bzero temp, halt

	load m1, 0xFF
	load m2, 0xFF
	load m3, 0xFF
	
	loadaddr pa, 0
	loadaddr idx, 0
	(pread)
	xor temp, idx, mask3
	bzero temp, pcorrectmod (if idx == 3, end of for loop)
	load cur, 0xFF
	sl temp, cur, idx 
	xor pa, pa, temp
	add idx, idx, one
	j pread
	
	(pcorrectmod)
	xor temp, pa, mask3
	bpos temp, flipm1 // ( mask != pa, go to next flip)
	xor m0, m0, 1
	
	(flipm1)
	xor temp, pa, mask5
	bpos temp, flipm2
	xor m1, m1, 1
	
	(flipm2)
	xor temp, pa, mask6
	bpos temp, flipm3
	xor m2, m2, 1
	
	(flipm3)
	xor temp, pa, mask7
	bpos temp, flipm0
	xor m3, m3, 1
	
	(print)
	store m0, 0xFF
	store m1, 0xFF
	store m2, 0xFF
	store m3, 0xFF
	j main
	
halt:
	0000
