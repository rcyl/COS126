#ifndef BSDELUXE_H_
#define BSDELUXE_H_

#include <vector>
#include <algorithm>

template <typename Key, class Compare>
static class BSDeluxe {

public:
	static int firstIndexOf(std::vector<Key> a, const Key &key, Compare comp);
	static int lastIndexOf(std::vector<Key> a, const Key &key, Compare comp);

};



#endif /* BSDELUXE_H_ */
