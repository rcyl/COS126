

/* register map */

r0 = 0
r1 = 1
r2 = pa
r3 = mask3
r4 
r5 = mask5
r6 = mask6
r7 = mask7
r8 = mask8
r9 = 
rA = 
rB =
rC = 
rD = 
rE = 
rF = 




loadaddr one, 1
loadaddr mask3, 3 
loadaddr mask5, 5
loadaddr mask6, 6
loadaddr mask7, 7
loadaddr mask8, 8 (0x1000) to check for negative numbers

main
	load m0, 0xFF
	and temp, m0, mask8 /* read first bit of input */
	bpos temp, halt

	load m1, 0xFF
	load m2, 0xFF
	load m3, 0xFF
	
	loadaddr pa, 0
	loadaddr idx, 0
	(pread)
	load cur, 0xFF
	sl temp, cur, idx 
	xor pa, pa, temp
	add idx, idx, one
	sub temp, mask3, idx
	bpos temp, pread
	
	(pcorrectmod)
	xor temp, pa, mask3
	bpos temp, flipm1 // ( mask != pa, go to next flip)
	xor m0, m0, 1
	
	(flipm1)
	xor temp, pa, mask5
	bpos temp, flipm2
	xor m1, m1, 1
	
	(flipm2)
	xor temp, pa, mask6
	bpos temp, flipm3
	xor m2, m2, 1
	
	(flipm3)
	xor temp, pa, mask7
	bpos temp, print
	xor m3, m3, 1
	
	(print)
	store m0, 0xFF
	store m1, 0xFF
	store m2, 0xFF
	store m3, 0xFF
	j main
	
halt:
	0000
